{
  "name": "plmjs",
  "version": "2.2.0",
  "description": "nodejs based plm application",
  "main": "index.js",
  "scripts": {
    "grunt": "grunt javascript_obfuscator",
    "start": "markdown-folder-to-html help/generalDoc&markdown-folder-to-html help/techDoc&gulp buildServer&nodemon index.js -q --delay 1",
    "deployVersion": "gulp writeCommit",
    "dev": "SET NODE_ENV=dev&&markdown-folder-to-html help/generalDoc&markdown-folder-to-html help/techDoc&markdown-folder-to-html help/internalDoc&gulp buildServer&nodemon index.js -q --delay 1",
    "refresh": "markdown-folder-to-html help/generalDoc&markdown-folder-to-html help/techDoc&markdown-folder-to-html help/internalDoc&gulp buildServer",
    "test": "NODE_ENV=test&&node startupScripts&&gulp buildServer&&mocha spec test -b --exit --recursive --timeout 20000",
    "testWithStats": "NODE_ENV=test&&node startupScripts&&gulp buildServer&&nyc mocha spec test -b --exit --recursive --timeout 20000",
    "testwin": "SET NODE_ENV=test&&node startupScripts&&gulp buildServer&&mocha spec test -b --exit --recursive --timeout 20000",
    "testwinWithStats": "SET NODE_ENV=test&&node startupScripts&&gulp buildServer&&nyc mocha spec test -b --exit --recursive --timeout 20000",
    "clientTest": "SET NODE_ENV=test&&mocha 'client.test.js' --exit --recursive --timeout 10000",
    "eslint": "eslint **/*.js --quiet",
    "eslintApi": "eslint api/**/*.js --quiet --ignore-pattern api/models/dm/**/*.js",
    "buildDatamodel": "gulp generateGanisterDatamodel",
    "docs": "markdown-folder-to-html help/generalDoc",
    "convertSass": "gulp sassTransform",
    "generateForms": "gulp forms",
    "coverage": "SET NODE_ENV=test&&nyc report --r html&mocha --exit --recursive --timeout 10000",
    "buildCustomMethods": "gulp customMethods",
    "postinstall": "gulp buildServer",
    "dbInit": "gulp generateGanisterDatamodel && node startupScripts",
    "export": "gulp exportApp",
    "ship": "gulp writeShippingCommit && gulp exportApp && grunt javascript_obfuscator && gulp zipDelivery",
    "dataLoader": "node dataloader/dataLoader.js",
    "upgrade": "node upgrade.js",
    "loadtest": "node loadtest.js",
    "testServer": "SET NODE_ENV=test&&markdown-folder-to-html help/generalDoc&markdown-folder-to-html help/techDoc&gulp buildServer&nodemon index.js -q --delay 1",
    "cypress:open": "SET NODE_ENV=test&&cypress open",
    "cypress:run": "SET NODE_ENV=test&&cypress run",
    "testdb:reset": "SET NODE_ENV=test&&node cypress/seeds/reset.js",
    "testdb:seed": "SET NODE_ENV=test&&node cypress/seeds/init.js&&node cypress/seeds/fixtures.js",
    "uploadApp:release": "npm config set plmjs:release true && gulp uploadAppToAWS",
    "uploadApp:dev": "npm config set plmjs:release false && gulp uploadAppToAWS",
    "getCommits": "gulp getCommits",
    "vite_dev": "vite",
    "vite_build": "vite build",
    "vite_preview": "vite preview",
    "eslint2": "eslint --ext .jsx --ext .js --ext .cjs ./src --fix"
  },
  "config": {
    "release": true
  },
  "nodemonConfig": {
    "ext": "js json css scss md",
    "ignore": [
      "assets/*",
      "log/*",
      "build/*",
      "api/models/datamodelDiff.json",
      "apps/*",
      "client/*",
      "help/_techDoc/*",
      "help/_generalDoc/*"
    ],
    "events": {
      "restart": "npm run refresh"
    }
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "keywords": [
    "PLM"
  ],
  "port": 8008,
  "author": "yma",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@octokit/rest": "^18.9.1",
    "adm-zip": "^0.4.16",
    "ag-grid-community": "21.1.0",
